{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "trailing-comma": [false, {
    "singleline": "never",
    "multiline": {
      "objects": "never",
      "arrays": "always",
      "functions": "never",
      "typeLiterals": "ignore"
    }
  }]
}
